{
  "name": "@roots/bud",
  "version": "6.12.3",
  "description": "Frontend build tools combining the best parts of Symfony Encore and Laravel Mix",
  "engines": {
    "node": ">=16.0.0"
  },
  "contributors": [
    {
      "name": "Kelly Mears",
      "email": "developers@tinypixel.dev",
      "url": "https://github.com/kellymears"
    },
    {
      "name": "Ben Word",
      "email": "ben@benword.com",
      "url": "https://github.com/retlehs"
    },
    {
      "name": "QWp6t",
      "url": "https://github.com/QWp6t"
    },
    {
      "name": "Brandon",
      "email": "brandon@tendency.me",
      "url": "https://github.com/Log1x"
    }
  ],
  "license": "MIT",
  "homepage": "https://roots.io/bud",
  "funding": {
    "type": "github sponsors",
    "url": "https://github.com/sponsors/roots"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/roots/bud.git",
    "directory": "sources/@roots/bud"
  },
  "bugs": "https://github.com/roots/bud/issues",
  "keywords": [
    "bud",
    "roots",
    "webpack"
  ],
  "bin": {
    "bud": "./bin/bud.mjs",
    "bund": "./bin/bund.mjs",
    "ts-bud": "./bin/ts-bud.mjs"
  },
  "files": [
    "bin",
    "docs",
    "lib",
    "config"
  ],
  "type": "module",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "default": "./lib/index.js"
    },
    "./bud": {
      "import": "./lib/bud/bud.js",
      "default": "./lib/bud/bud.js"
    },
    "./instances": {
      "import": "./lib/instances.js",
      "default": "./lib/instances.js"
    },
    "./cli/*": {
      "import": "./lib/cli/*.js",
      "default": "./lib/cli/*.js"
    },
    "./cli/commands/*": {
      "import": "./lib/cli/commands/*.js",
      "default": "./lib/cli/commands/*.js"
    },
    "./cli/decorators": {
      "import": "./lib/cli/decorators/index.js",
      "default": "./lib/cli/decorators/index.js"
    },
    "./cli/decorators/*": {
      "import": "./lib/cli/decorators/*.js",
      "default": "./lib/cli/decorators/*.js"
    },
    "./cli/helpers/*": {
      "import": "./lib/cli/helpers/*.js",
      "default": "./lib/cli/helpers/*.js"
    },
    "./context": {
      "import": "./lib/context/index.js",
      "default": "./lib/context/index.js"
    },
    "./context/*": {
      "import": "./lib/context/*.js",
      "default": "./lib/context/*.js"
    },
    "./factory": {
      "import": "./lib/factory/index.js",
      "default": "./lib/factory/index.js"
    },
    "./factory/*": {
      "import": "./lib/factory/*.js",
      "default": "./lib/factory/*.js"
    },
    "./services/*": {
      "import": "./lib/services/*/index.js",
      "default": "./lib/services/*/index.js"
    },
    "./services/*/*": {
      "import": "./lib/services/*/*.js",
      "default": "./lib/services/*/*.js"
    },
    "./types": {
      "import": "./lib/types.js",
      "default": "./lib/types.js"
    },
    "./config/jsconfig.json": "./config/jsconfig.json",
    "./config/tsconfig.json": "./config/tsconfig.json"
  },
  "typesVersions": {
    "*": {
      ".": [
        "./lib/index.d.ts"
      ],
      "cli": [
        "./lib/cli/index.d.ts"
      ],
      "cli/commands": [
        "./lib/cli/commands/index.d.ts"
      ],
      "cli/decorators": [
        "./lib/cli/decorators/index.d.ts"
      ],
      "context": [
        "./lib/context/index.d.ts"
      ],
      "factory": [
        "./lib/factory/index.d.ts"
      ],
      "factory/*": [
        "./lib/factory/*.d.ts"
      ],
      "services": [
        "./lib/services/index.d.ts"
      ],
      "services/*": [
        "./lib/services/*/index.d.ts"
      ],
      "types": [
        "./lib/types.d.ts"
      ]
    }
  },
  "types": "./lib/index.d.ts",
  "module": "./lib/index.js",
  "devDependencies": {
    "@skypack/package-check": "0.2.2",
    "@types/node": "18.16.6",
    "@types/react": "18.2.6"
  },
  "dependencies": {
    "@roots/browserslist-config": "6.12.3",
    "@roots/bud-api": "6.12.3",
    "@roots/bud-build": "6.12.3",
    "@roots/bud-cache": "6.12.3",
    "@roots/bud-client": "6.12.3",
    "@roots/bud-compiler": "6.12.3",
    "@roots/bud-dashboard": "6.12.3",
    "@roots/bud-entrypoints": "6.12.3",
    "@roots/bud-extensions": "6.12.3",
    "@roots/bud-framework": "6.12.3",
    "@roots/bud-hooks": "6.12.3",
    "@roots/bud-server": "6.12.3",
    "@roots/bud-support": "6.12.3",
    "@roots/bud-terser": "6.12.3",
    "chalk": "5.2.0",
    "ink": "4.2.0",
    "ink-spinner": "5.0.0",
    "ink-text-input": "5.0.1",
    "react": "18.2.0",
    "tslib": "2.5.0"
  },
  "volta": {
    "extends": "../../../package.json"
  }
}